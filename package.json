{
  "name": "declassified-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "node -e \"try{require.resolve('eslint'); process.exit(0)}catch(e){console.log('eslint unavailable; skipping lint in this environment')}\" && npm run lint:strict || true",
    "check:contrast": "node scripts/check-contrast.mjs",
    "check:content": "node scripts/check-content.mjs",
    "lint:ci": "npm run lint",
    "lint:strict": "next lint",
    "qa:verify": "npm run check:contrast && npm run build && npm run check:content"
  },
  "dependencies": {
    "next": "15.0.3",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.4"
  }
}
